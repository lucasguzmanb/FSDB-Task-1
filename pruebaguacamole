//Para loans debería tener(id, start_date, return_date, user_id, copy_id, returned, real_return_date)

SELECT * FROM fsdb.loans WHERE TRIM(RETURN) IS NULL;
SELECT date_time, return, user_id FROM  fsdb.loans;


//Para Sanctions debería tener(id, user_id, loan_id, weeks, start_date, end_date)

    //Con esto selecciono las que tienen sanction
SELECT date_time, return, user_id 
FROM fsdb.loans 
WHERE RETURN IS NOT NULL 
AND TO_DATE(RETURN, 'DD/MM/YYYY HH24:MI:SS') - TO_DATE(DATE_TIME, 'DD/MM/YYYY HH24:MI:SS') > 14; 


//Para users debería tener(user_id, name, surname, passport, birthdate, municipality_id, adress, email, telephone, user_type)



